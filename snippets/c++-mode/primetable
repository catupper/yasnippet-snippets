# -*- mode: snippet -*-
# name: PrimeTable
# key: /primetable
# group: NumberTheory
# --


bool isPrime[1080000];
void init(){
  fill(isPrime, isPrime + 1080000, true);
  isPrime[0] = isPrime[1] = false;
  for(int i = 2;i*i < 1080000;i++){
    if(!isPrime[i])continue;
    for(int j = i * i;j < 1080000;j += i)isPrime[j] = false;
  }
}
